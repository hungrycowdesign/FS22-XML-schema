<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:simpleType name="g_hyphen">
        <xs:restriction base="xs:string">
            <xs:pattern value="-"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="g_float_or_hyphen">
        <xs:union memberTypes="xs:float g_hyphen"/>
    </xs:simpleType>

    <xs:simpleType name="g_vector_float">
        <xs:list itemType="g_float_or_hyphen"/>
    </xs:simpleType>

    <xs:simpleType name="g_string">
        <xs:annotation><xs:appinfo><typeStr>String</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:string"/>
        <xs:annotation><xs:appinfo><typeStr>String</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_l10n_string">
        <xs:annotation><xs:appinfo><typeStr>String or l10n key</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:string"/>
        <xs:annotation><xs:appinfo><typeStr>String or l10n key</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_float">
        <xs:annotation><xs:appinfo><typeStr>Float</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:float"/>
        <xs:annotation><xs:appinfo><typeStr>Float</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_angle">
        <xs:annotation><xs:appinfo><typeStr>Angle</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:float"/>
        <xs:annotation><xs:appinfo><typeStr>Angle</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_time">
        <xs:annotation><xs:appinfo><typeStr>Time in seconds</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:float"/>
        <xs:annotation><xs:appinfo><typeStr>Time in seconds</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_int">
        <xs:annotation><xs:appinfo><typeStr>Integer</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:integer"/>
        <xs:annotation><xs:appinfo><typeStr>Integer</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_bool">
        <xs:annotation><xs:appinfo><typeStr>Boolean</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="true|false"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Boolean</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_node_index">
        <xs:annotation><xs:appinfo><typeStr>Index to i3d node or i3d mapping identifier</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:string"/>
        <xs:annotation><xs:appinfo><typeStr>Index to i3d node or i3d mapping identifier</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_node_indices">
        <xs:annotation><xs:appinfo><typeStr>List of indices to i3d nodes or i3d mapping identifiers</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:string"/>
        <xs:annotation><xs:appinfo><typeStr>List of indices to i3d nodes or i3d mapping identifiers</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_2">
        <xs:annotation><xs:appinfo><typeStr>Multiple values (x, y)</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float">
            <xs:pattern value="\S+ \S+"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Multiple values (x, y)</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_3">
        <xs:annotation><xs:appinfo><typeStr>Multiple values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float">
            <xs:pattern value="\S+ \S+ \S+"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Multiple values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_4">
        <xs:annotation><xs:appinfo><typeStr>Multiple values (x, y, z, w)</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float">
            <xs:pattern value="\S+ \S+ \S+ \S+"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Multiple values (x, y, z, w)</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_n">
        <xs:annotation><xs:appinfo><typeStr>Multiple values</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float"/>
        <xs:annotation><xs:appinfo><typeStr>Multiple values</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_trans">
        <xs:annotation><xs:appinfo><typeStr>Translation values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float">
            <xs:pattern value="\S+ \S+ \S+"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Translation values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_rot">
        <xs:annotation><xs:appinfo><typeStr>Rotation values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float">
            <xs:pattern value="\S+ \S+ \S+"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Rotation values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_rot_2">
        <xs:annotation><xs:appinfo><typeStr>Rotation values (x, y)</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float">
            <xs:pattern value="\S+ \S+"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Rotation values (x, y)</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_scale">
        <xs:annotation><xs:appinfo><typeStr>Scale values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float">
            <xs:pattern value="\S+ \S+ \S+"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Scale values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_color">
        <xs:annotation><xs:appinfo><typeStr>Color values (r, g, b) or brand color id</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:string"/>
        <xs:annotation><xs:appinfo><typeStr>Color values (r, g, b) or brand color id</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:element name="placeables">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element name="parentFile" minOccurs="0" maxOccurs="1">
                    <xs:complexType>
                        <xs:all>
                            <xs:element name="attributes" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:choice maxOccurs="unbounded">
                                        <xs:element name="clearList" minOccurs="0" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:attribute name="keepIndex" type="g_int">
                                                    <xs:annotation><xs:documentation xml:lang="en">Index of list to keep</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="path" type="g_string">
                                                    <xs:annotation><xs:documentation xml:lang="en">List to clear but keep one item</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="remove" minOccurs="0" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:attribute name="path" type="g_string">
                                                    <xs:annotation><xs:documentation xml:lang="en">Path to remove from parent xml</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="set" minOccurs="0" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:attribute name="path" type="g_string">
                                                    <xs:annotation><xs:documentation xml:lang="en">Path change in parent xml</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="value" type="g_string">
                                                    <xs:annotation><xs:documentation xml:lang="en">Target value to set in parent file</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:choice>
                                </xs:complexType>
                            </xs:element>
                        </xs:all>
                        <xs:attribute name="xmlFilename" type="g_string">
                            <xs:annotation><xs:documentation xml:lang="en">Remove vehicle if unit empty</xs:documentation></xs:annotation>
                        </xs:attribute>
                    </xs:complexType>
                </xs:element>
                <xs:element name="placeable" minOccurs="0" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:all>
                            <xs:element name="animatedObjects" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:choice maxOccurs="unbounded">
                                        <xs:element name="animatedObject" minOccurs="0" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:attribute name="direction" type="g_int" default="0">
                                                    <xs:annotation><xs:documentation xml:lang="en">Animated object direction</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="time" type="g_float">
                                                    <xs:annotation><xs:documentation xml:lang="en">Animated object time</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:choice>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="beehivePalletSpawner" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:all>
                                        <xs:element name="beehivePalletSpawner" minOccurs="0" maxOccurs="1">
                                            <xs:complexType>
                                                <xs:attribute name="pendingLiters" type="g_float">
                                                    <xs:annotation><xs:documentation xml:lang="en">Pending liters to be spawned</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:all>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="bunkerSilo" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:attribute name="compactedFillLevel" type="g_float">
                                        <xs:annotation><xs:documentation xml:lang="en">Compacted fill level</xs:documentation></xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="fermentingTime" type="g_float">
                                        <xs:annotation><xs:documentation xml:lang="en">Fermenting time</xs:documentation></xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="fillLevel" type="g_float">
                                        <xs:annotation><xs:documentation xml:lang="en">Current fill level</xs:documentation></xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="openedAtBack" type="g_bool" default="false">
                                        <xs:annotation><xs:documentation xml:lang="en">Is opened at back</xs:documentation></xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="openedAtFront" type="g_bool" default="false">
                                        <xs:annotation><xs:documentation xml:lang="en">Is opened at front</xs:documentation></xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="state" type="g_int" default="0">
                                        <xs:annotation><xs:documentation xml:lang="en">Current silo state (FILL = 0, CLOSED = 1, FERMENTED = 2, DRAIN = 3)</xs:documentation></xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="colorable" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:all>
                                        <xs:element name="color" type="g_int" minOccurs="0">
                                            <xs:annotation><xs:documentation xml:lang="en">Active color index</xs:documentation></xs:annotation>
                                        </xs:element>
                                    </xs:all>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="doghouse" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:attribute name="name" type="g_string">
                                        <xs:annotation><xs:documentation xml:lang="en">Name of dog</xs:documentation></xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="fence" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:all>
                                        <xs:element name="segments" minOccurs="0" maxOccurs="1">
                                            <xs:complexType>
                                                <xs:choice maxOccurs="unbounded">
                                                    <xs:element name="segment" minOccurs="0" maxOccurs="unbounded">
                                                        <xs:complexType>
                                                            <xs:all>
                                                                <xs:element name="animatedObject" minOccurs="0" maxOccurs="1">
                                                                    <xs:complexType>
                                                                        <xs:attribute name="direction" type="g_int" default="0">
                                                                            <xs:annotation><xs:documentation xml:lang="en">Animated object direction</xs:documentation></xs:annotation>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="time" type="g_float">
                                                                            <xs:annotation><xs:documentation xml:lang="en">Animated object time</xs:documentation></xs:annotation>
                                                                        </xs:attribute>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                            </xs:all>
                                                            <xs:attribute name="end" type="g_vector_2">
                                                                <xs:annotation><xs:documentation xml:lang="en">Segment end position</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="first" type="g_bool" default="true">
                                                                <xs:annotation><xs:documentation xml:lang="en">Segment has first pole visible</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="gateIndex" type="g_int">
                                                                <xs:annotation><xs:documentation xml:lang="en">Gate index</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="last" type="g_bool" default="true">
                                                                <xs:annotation><xs:documentation xml:lang="en">Segment has last pole visible</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="start" type="g_vector_2">
                                                                <xs:annotation><xs:documentation xml:lang="en">Segment start position</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:choice>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:all>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="husbandry" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:choice maxOccurs="unbounded">
                                        <xs:element name="module" minOccurs="0" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:attribute name="name" type="g_string">
                                                    <xs:annotation><xs:documentation xml:lang="en">Name of module</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="storage" minOccurs="0" maxOccurs="1">
                                            <xs:complexType>
                                                <xs:choice maxOccurs="unbounded">
                                                    <xs:element name="node" minOccurs="0" maxOccurs="unbounded">
                                                        <xs:complexType>
                                                            <xs:attribute name="fillLevel" type="g_float" default="0">
                                                                <xs:annotation><xs:documentation xml:lang="en">Fill level</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="fillType" type="g_string">
                                                                <xs:annotation><xs:documentation xml:lang="en">Fill type name</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:choice>
                                                <xs:attribute name="farmId" type="g_int" default="0">
                                                    <xs:annotation><xs:documentation xml:lang="en">Owner farm land id</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:choice>
                                    <xs:attribute name="globalProductionFactor" type="g_float">
                                        <xs:annotation><xs:documentation xml:lang="en">Global production factor</xs:documentation></xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="husbandryFeedingRobot" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:choice maxOccurs="unbounded">
                                        <xs:element name="unloadingSpot" minOccurs="0" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:attribute name="fillLevel" type="g_int">
                                                    <xs:annotation><xs:documentation xml:lang="en">Unloading spot filllevel</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="index" type="g_int">
                                                    <xs:annotation><xs:documentation xml:lang="en">Unloading spot index</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:choice>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="husbandryFood" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:choice maxOccurs="unbounded">
                                        <xs:element name="fillLevel" minOccurs="0" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:attribute name="fillLevel" type="g_float">
                                                    <xs:annotation><xs:documentation xml:lang="en">Fill level</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="fillType" type="g_string">
                                                    <xs:annotation><xs:documentation xml:lang="en">Fill type</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:choice>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="husbandryPallets" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:attribute name="fillLevel" type="g_float">
                                        <xs:annotation><xs:documentation xml:lang="en">Fill Level</xs:documentation></xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="pendingLiters" type="g_float">
                                        <xs:annotation><xs:documentation xml:lang="en">Pending liters</xs:documentation></xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="manureHeap" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:attribute name="manureToDrop" type="g_int" default="0">
                                        <xs:annotation><xs:documentation xml:lang="en">Manure that should be drop the visible heap</xs:documentation></xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="manureToPick" type="g_int" default="0">
                                        <xs:annotation><xs:documentation xml:lang="en">Manure that need to be picked from visible heap</xs:documentation></xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="productionPoint" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:choice maxOccurs="unbounded">
                                        <xs:element name="autoDeliverFillType" type="g_string" minOccurs="0">
                                            <xs:annotation><xs:documentation xml:lang="en">fillType currently configured to be automatically delivered</xs:documentation></xs:annotation>
                                        </xs:element>
                                        <xs:element name="directSellFillType" type="g_string" minOccurs="0">
                                            <xs:annotation><xs:documentation xml:lang="en">fillType currently configured to be directly sold</xs:documentation></xs:annotation>
                                        </xs:element>
                                        <xs:element name="production" minOccurs="0" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:attribute name="id" type="g_string">
                                                    <xs:annotation><xs:documentation xml:lang="en">Unique id of the production</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="isEnabled" type="g_bool">
                                                    <xs:annotation><xs:documentation xml:lang="en">State of the production</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="storage" minOccurs="0" maxOccurs="1">
                                            <xs:complexType>
                                                <xs:choice maxOccurs="unbounded">
                                                    <xs:element name="node" minOccurs="0" maxOccurs="unbounded">
                                                        <xs:complexType>
                                                            <xs:attribute name="fillLevel" type="g_float" default="0">
                                                                <xs:annotation><xs:documentation xml:lang="en">Fill level</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="fillType" type="g_string">
                                                                <xs:annotation><xs:documentation xml:lang="en">Fill type name</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:choice>
                                                <xs:attribute name="farmId" type="g_int" default="0">
                                                    <xs:annotation><xs:documentation xml:lang="en">Owner farm land id</xs:documentation></xs:annotation>
                                                </xs:attribute>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:choice>
                                    <xs:attribute name="palletSpawnCooldown" type="g_int">
                                        <xs:annotation><xs:documentation xml:lang="en">remaining cooldown duration of pallet spawner</xs:documentation></xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="productionCostsToClaim" type="g_float">
                                        <xs:annotation><xs:documentation xml:lang="en">production costs yet to be claimed from the owning player</xs:documentation></xs:annotation>
                                    </xs:attribute>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="sellingStation" minOccurs="0" maxOccurs="1">
                                <xs:complexType>
                                    <xs:choice maxOccurs="unbounded">
                                        <xs:element name="stats" minOccurs="0" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:all>
                                                    <xs:element name="curve1" minOccurs="0" maxOccurs="1">
                                                        <xs:complexType>
                                                            <xs:attribute name="amplitude" type="g_float">
                                                                <xs:annotation><xs:documentation xml:lang="en">Amplitude</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="amplitudeDistribution" type="g_int">
                                                                <xs:annotation><xs:documentation xml:lang="en">Amplitude fistribution</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="nominalAmplitude" type="g_float">
                                                                <xs:annotation><xs:documentation xml:lang="en">Normal amplitude</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="nominalAmplitudeVariation" type="g_float">
                                                                <xs:annotation><xs:documentation xml:lang="en">Normal amplitude variation</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="nominalPeriod" type="g_int">
                                                                <xs:annotation><xs:documentation xml:lang="en">Nominal period</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="nominalPeriodVariation" type="g_int">
                                                                <xs:annotation><xs:documentation xml:lang="en">Nominal period variation</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="period" type="g_float">
                                                                <xs:annotation><xs:documentation xml:lang="en">Period</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="periodDistribution" type="g_int">
                                                                <xs:annotation><xs:documentation xml:lang="en">Period distribution</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="time" type="g_float">
                                                                <xs:annotation><xs:documentation xml:lang="en">Time</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="curveBaseCurve" minOccurs="0" maxOccurs="1">
                                                        <xs:complexType>
                                                            <xs:attribute name="amplitude" type="g_float">
                                                                <xs:annotation><xs:documentation xml:lang="en">Amplitude</xs:documentation></xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="amplitudeDistribution" type="g_int">
                                                                <xs:annotation><xs:documentation xml:lang="en">Amplitude fistribution</xs:documentation></xs:annotation>
