<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:simpleType name="g_hyphen">
        <xs:restriction base="xs:string">
            <xs:pattern value="-"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="g_float_or_hyphen">
        <xs:union memberTypes="xs:float g_hyphen"/>
    </xs:simpleType>

    <xs:simpleType name="g_vector_float">
        <xs:list itemType="g_float_or_hyphen"/>
    </xs:simpleType>

    <xs:simpleType name="g_string">
        <xs:annotation><xs:appinfo><typeStr>String</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:string"/>
        <xs:annotation><xs:appinfo><typeStr>String</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_l10n_string">
        <xs:annotation><xs:appinfo><typeStr>String or l10n key</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:string"/>
        <xs:annotation><xs:appinfo><typeStr>String or l10n key</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_float">
        <xs:annotation><xs:appinfo><typeStr>Float</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:float"/>
        <xs:annotation><xs:appinfo><typeStr>Float</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_angle">
        <xs:annotation><xs:appinfo><typeStr>Angle</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:float"/>
        <xs:annotation><xs:appinfo><typeStr>Angle</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_time">
        <xs:annotation><xs:appinfo><typeStr>Time in seconds</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:float"/>
        <xs:annotation><xs:appinfo><typeStr>Time in seconds</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_int">
        <xs:annotation><xs:appinfo><typeStr>Integer</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:integer"/>
        <xs:annotation><xs:appinfo><typeStr>Integer</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_bool">
        <xs:annotation><xs:appinfo><typeStr>Boolean</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="true|false"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Boolean</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_node_index">
        <xs:annotation><xs:appinfo><typeStr>Index to i3d node or i3d mapping identifier</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:string"/>
        <xs:annotation><xs:appinfo><typeStr>Index to i3d node or i3d mapping identifier</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_node_indices">
        <xs:annotation><xs:appinfo><typeStr>List of indices to i3d nodes or i3d mapping identifiers</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:string"/>
        <xs:annotation><xs:appinfo><typeStr>List of indices to i3d nodes or i3d mapping identifiers</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_2">
        <xs:annotation><xs:appinfo><typeStr>Multiple values (x, y)</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float">
            <xs:pattern value="\S+ \S+"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Multiple values (x, y)</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_3">
        <xs:annotation><xs:appinfo><typeStr>Multiple values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float">
            <xs:pattern value="\S+ \S+ \S+"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Multiple values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_4">
        <xs:annotation><xs:appinfo><typeStr>Multiple values (x, y, z, w)</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float">
            <xs:pattern value="\S+ \S+ \S+ \S+"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Multiple values (x, y, z, w)</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_n">
        <xs:annotation><xs:appinfo><typeStr>Multiple values</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float"/>
        <xs:annotation><xs:appinfo><typeStr>Multiple values</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_trans">
        <xs:annotation><xs:appinfo><typeStr>Translation values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float">
            <xs:pattern value="\S+ \S+ \S+"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Translation values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_rot">
        <xs:annotation><xs:appinfo><typeStr>Rotation values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float">
            <xs:pattern value="\S+ \S+ \S+"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Rotation values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_rot_2">
        <xs:annotation><xs:appinfo><typeStr>Rotation values (x, y)</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float">
            <xs:pattern value="\S+ \S+"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Rotation values (x, y)</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_vector_scale">
        <xs:annotation><xs:appinfo><typeStr>Scale values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="g_vector_float">
            <xs:pattern value="\S+ \S+ \S+"/>
        </xs:restriction>
        <xs:annotation><xs:appinfo><typeStr>Scale values (x, y, z)</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:simpleType name="g_color">
        <xs:annotation><xs:appinfo><typeStr>Color values (r, g, b) or brand color id</typeStr></xs:appinfo></xs:annotation>
        <xs:restriction base="xs:string"/>
        <xs:annotation><xs:appinfo><typeStr>Color values (r, g, b) or brand color id</typeStr></xs:appinfo></xs:annotation>
    </xs:simpleType>

    <xs:complexType name="configAIAgent">
        <xs:attribute name="frontOffset" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">front offset</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="height" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">ai height of the vehicle when loaded in this configuration</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="length" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">ai length of the vehicle when loaded in this configuration</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="lengthOffset" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">length offset</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="maxBrakeAcceleration" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">AI vehicle max brake acceleration</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="maxCentripedalAcceleration" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">AI vehicle max centripedal acceleration</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="maxTurningRadius" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">Max. turning radius (overwrites value detected from ackermann steering)</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="width" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">ai width of the vehicle when loaded in this configuration</xs:documentation></xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="foliageBendingModifier">
        <xs:attribute name="index" type="g_int">
            <xs:annotation><xs:documentation xml:lang="en">Bending node index</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="indices" type="g_vector_n">
            <xs:annotation><xs:documentation xml:lang="en">Bending node indices</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="isActive" type="g_bool" default="true">
            <xs:annotation><xs:documentation xml:lang="en">Bending node is active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="maxX" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">Max. width</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="maxZ" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">Max. length</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="minX" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">Min. width</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="minZ" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">Min. length</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="overwrite" type="g_bool" default="true">
            <xs:annotation><xs:documentation xml:lang="en">Overwrite the bending node values and do not use the max values</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="yOffset" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">Y translation offset</xs:documentation></xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="ObjectChange_single">
        <xs:attribute name="centerOfMassActive" type="g_vector_3">
            <xs:annotation><xs:documentation xml:lang="en">center of mass if object change is active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="centerOfMassInactive" type="g_vector_3">
            <xs:annotation><xs:documentation xml:lang="en">center of mass if object change is in active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="compoundChildActive" type="g_bool">
            <xs:annotation><xs:documentation xml:lang="en">compound child state if object change is active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="compoundChildInactive" type="g_bool">
            <xs:annotation><xs:documentation xml:lang="en">compound child state if object change is in active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="interpolation" type="g_bool" default="false">
            <xs:annotation><xs:documentation xml:lang="en">Value will be interpolated</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="interpolationTime" type="g_time" default="1">
            <xs:annotation><xs:documentation xml:lang="en">Time for interpolation</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="massActive" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">mass if object change is active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="massInactive" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">mass if object change is in active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="movingPartUpdateActive" type="g_bool">
            <xs:annotation><xs:documentation xml:lang="en">moving part active state if object change active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="movingPartUpdateInactive" type="g_bool">
            <xs:annotation><xs:documentation xml:lang="en">moving part active state if object change inactive</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="movingToolRotMaxActive" type="g_angle">
            <xs:annotation><xs:documentation xml:lang="en">Moving tool max. rotation if object change active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="movingToolRotMaxInactive" type="g_angle">
            <xs:annotation><xs:documentation xml:lang="en">Moving tool max. rotation if object change inactive</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="movingToolRotMinActive" type="g_angle">
            <xs:annotation><xs:documentation xml:lang="en">Moving tool min. rotation if object change active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="movingToolRotMinInactive" type="g_angle">
            <xs:annotation><xs:documentation xml:lang="en">Moving tool min. rotation if object change inactive</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="movingToolTransMaxActive" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">Moving tool max. translation if object change active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="movingToolTransMaxInactive" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">Moving tool max. translation if object change inactive</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="movingToolTransMinActive" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">Moving tool min. translation if object change active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="movingToolTransMinInactive" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">Moving tool min. translation if object change inactive</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="node" type="g_node_index">
            <xs:annotation><xs:documentation xml:lang="en">Object change node</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="parentNodeActive" type="g_node_index">
            <xs:annotation><xs:documentation xml:lang="en">parent node if object change is active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="parentNodeInactive" type="g_node_index">
            <xs:annotation><xs:documentation xml:lang="en">parent node if object change is in active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="rigidBodyTypeActive" type="g_string">
            <xs:annotation><xs:documentation xml:lang="en">rigid body type if object change is active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="rigidBodyTypeInactive" type="g_string">
            <xs:annotation><xs:documentation xml:lang="en">rigid body type if object change is in active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="rotationActive" type="g_vector_rot">
            <xs:annotation><xs:documentation xml:lang="en">rotation if object change is active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="rotationInactive" type="g_vector_rot">
            <xs:annotation><xs:documentation xml:lang="en">rotation if object change is in active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="scaleActive" type="g_vector_scale">
            <xs:annotation><xs:documentation xml:lang="en">scale if object change is active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="scaleInactive" type="g_vector_scale">
            <xs:annotation><xs:documentation xml:lang="en">scale if object change is in active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="shaderParameter" type="g_string">
            <xs:annotation><xs:documentation xml:lang="en">Shader parameter name</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="shaderParameterActive" type="g_vector_4">
            <xs:annotation><xs:documentation xml:lang="en">shaderParameter if object change is active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="shaderParameterInactive" type="g_vector_4">
            <xs:annotation><xs:documentation xml:lang="en">shaderParameter if object change is in active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="sharedShaderParameter" type="g_bool" default="false">
            <xs:annotation><xs:documentation xml:lang="en">Shader parameter is applied on all objects with the same material</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="translationActive" type="g_vector_trans">
            <xs:annotation><xs:documentation xml:lang="en">translation if object change is active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="translationInactive" type="g_vector_trans">
            <xs:annotation><xs:documentation xml:lang="en">translation if object change is in active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="visibilityActive" type="g_bool">
            <xs:annotation><xs:documentation xml:lang="en">visibility if object change is active</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="visibilityInactive" type="g_bool">
            <xs:annotation><xs:documentation xml:lang="en">visibility if object change is in active</xs:documentation></xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="configReverseDriving">
        <xs:attribute name="isAllowed" type="g_bool" default="true">
            <xs:annotation><xs:documentation xml:lang="en">Reverse driving is allowed while this configuration is equipped</xs:documentation></xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="configSize">
        <xs:attribute name="height" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">occupied height of the vehicle when loaded in this configuration</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="heightOffset" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">height offset</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="length" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">occupied length of the vehicle when loaded in this configuration</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="lengthOffset" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">length offset</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="width" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">occupied width of the vehicle when loaded in this configuration</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="widthOffset" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">width offset</xs:documentation></xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="SoundManager_sound">
        <xs:choice maxOccurs="unbounded">
            <xs:element name="loopSynthesisLoad" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:choice maxOccurs="unbounded">
                        <xs:element name="modifier" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="modifiedValue" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">Change that is applied on sample value</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="type" type="g_string">
                                    <xs:annotation><xs:documentation xml:lang="en">Modifier type</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="value" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">Source value of modifier type</xs:documentation></xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:choice>
                </xs:complexType>
            </xs:element>
            <xs:element name="loopSynthesisRpm" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:choice maxOccurs="unbounded">
                        <xs:element name="modifier" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="modifiedValue" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">Change that is applied on sample value</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="type" type="g_string">
                                    <xs:annotation><xs:documentation xml:lang="en">Modifier type</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="value" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">Source value of modifier type</xs:documentation></xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:choice>
                </xs:complexType>
            </xs:element>
            <xs:element name="lowpassCutoffFrequency" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:attribute name="indoor" type="g_float" default="5000">
                        <xs:annotation><xs:documentation xml:lang="en">Indoor lowpass cutoff frequency</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="outdoor" type="g_float" default="5000">
                        <xs:annotation><xs:documentation xml:lang="en">Outdoor lowpass cutoff frequency</xs:documentation></xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="lowpassGain" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:choice maxOccurs="unbounded">
                        <xs:element name="modifier" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="modifiedValue" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">Change that is applied on sample value</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="type" type="g_string">
                                    <xs:annotation><xs:documentation xml:lang="en">Modifier type</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="value" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">Source value of modifier type</xs:documentation></xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:choice>
                    <xs:attribute name="indoor" type="g_float" default="0.8">
                        <xs:annotation><xs:documentation xml:lang="en">Indoor lowpass gain</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="outdoor" type="g_float" default="1">
                        <xs:annotation><xs:documentation xml:lang="en">Outdoor lowpass gain</xs:documentation></xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="lowpassResonance" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:attribute name="indoor" type="g_float" default="2">
                        <xs:annotation><xs:documentation xml:lang="en">Indoor lowpass resonance</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="outdoor" type="g_float" default="2">
                        <xs:annotation><xs:documentation xml:lang="en">Outdoor lowpass resonance</xs:documentation></xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="pitch" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:choice maxOccurs="unbounded">
                        <xs:element name="modifier" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="modifiedValue" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">Change that is applied on sample value</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="type" type="g_string">
                                    <xs:annotation><xs:documentation xml:lang="en">Modifier type</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="value" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">Source value of modifier type</xs:documentation></xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:choice>
                    <xs:attribute name="indoor" type="g_float" default="1">
                        <xs:annotation><xs:documentation xml:lang="en">Indoor pitch</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="outdoor" type="g_float" default="1">
                        <xs:annotation><xs:documentation xml:lang="en">Outdoor pitch</xs:documentation></xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="randomization" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:attribute name="isInside" type="g_bool" default="true">
                        <xs:annotation><xs:documentation xml:lang="en">Randomization is applied inside</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="isOutside" type="g_bool" default="true">
                        <xs:annotation><xs:documentation xml:lang="en">Randomization is applied outside</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="maxLowpassGain" type="g_float">
                        <xs:annotation><xs:documentation xml:lang="en">Max lowpass gain</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="maxPitch" type="g_float">
                        <xs:annotation><xs:documentation xml:lang="en">Max pitch</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="maxVolume" type="g_float">
                        <xs:annotation><xs:documentation xml:lang="en">Max volume</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="minLowpassGain" type="g_float">
                        <xs:annotation><xs:documentation xml:lang="en">Max lowpass gain</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="minPitch" type="g_float">
                        <xs:annotation><xs:documentation xml:lang="en">Max pitch</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="minVolume" type="g_float">
                        <xs:annotation><xs:documentation xml:lang="en">Min volume</xs:documentation></xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="sourceRandomization" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:attribute name="file" type="g_string">
                        <xs:annotation><xs:documentation xml:lang="en">Path to sound sample</xs:documentation></xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
            <xs:element name="volume" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:choice maxOccurs="unbounded">
                        <xs:element name="modifier" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="modifiedValue" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">Change that is applied on sample value</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="type" type="g_string">
                                    <xs:annotation><xs:documentation xml:lang="en">Modifier type</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="value" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">Source value of modifier type</xs:documentation></xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:choice>
                    <xs:attribute name="indoor" type="g_float" default="0.8">
                        <xs:annotation><xs:documentation xml:lang="en">Indoor volume</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="outdoor" type="g_float" default="1">
                        <xs:annotation><xs:documentation xml:lang="en">Outdoor volume</xs:documentation></xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:choice>
        <xs:attribute name="debug" type="g_bool" default="false">
            <xs:annotation><xs:documentation xml:lang="en">Flag to enable debug rendering</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="fadeIn" type="g_float" default="0">
            <xs:annotation><xs:documentation xml:lang="en">Fade in time in seconds</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="fadeOut" type="g_float" default="0">
            <xs:annotation><xs:documentation xml:lang="en">Fade out time in seconds</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="file" type="g_string">
            <xs:annotation><xs:documentation xml:lang="en">Path to sound sample</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="innerRadius" type="g_float" default="80">
            <xs:annotation><xs:documentation xml:lang="en">Inner radius</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="isLocalSound" type="g_bool" default="false">
            <xs:annotation><xs:documentation xml:lang="en">While set for vehicle sounds it will only play for the player currently using the vehicle</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="linkNode" type="g_node_index">
            <xs:annotation><xs:documentation xml:lang="en">Link node for 3d sound</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="linkNodeOffset" type="g_vector_trans">
            <xs:annotation><xs:documentation xml:lang="en">Sound source will be offset by this value to the link node</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="loops" type="g_int" default="1">
            <xs:annotation><xs:documentation xml:lang="en">Number of loops (0 = infinite)</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="loopSynthesisRPMRatio" type="g_float" default="1">
            <xs:annotation><xs:documentation xml:lang="en">Ratio between rpm in the gls file and actual rpm of the motor (e.g. 0.9: max. rpm in the gls file will be reached at 90% of motor rpm)</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="lowpassGainScale" type="g_float" default="1">
            <xs:annotation><xs:documentation xml:lang="en">Additional lowpass gain that is applied on the volume attributes</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="outerRadius" type="g_float" default="5">
            <xs:annotation><xs:documentation xml:lang="en">Outer radius</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="parent" type="g_string">
            <xs:annotation><xs:documentation xml:lang="en">Parent sample for heredity</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="pitchScale" type="g_float" default="1">
            <xs:annotation><xs:documentation xml:lang="en">Additional pitch that is applied on the volume attributes</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="supportsReverb" type="g_bool" default="true">
            <xs:annotation><xs:documentation xml:lang="en">Flag to disable reverb</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="template" type="g_string">
            <xs:annotation><xs:documentation xml:lang="en">Sound template name</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="volumeScale" type="g_float" default="1">
            <xs:annotation><xs:documentation xml:lang="en">Additional scale that is applied on the volume attributes</xs:documentation></xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="AnimationNode">
        <xs:all>
            <xs:element name="spikes" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:choice maxOccurs="unbounded">
                        <xs:element name="spike" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="direction" type="g_int">
                                    <xs:annotation><xs:documentation xml:lang="en">Spike rot. direction</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="node" type="g_node_index">
                                    <xs:annotation><xs:documentation xml:lang="en">Spike node</xs:documentation></xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:choice>
                    <xs:attribute name="maxRot" type="g_angle">
                        <xs:annotation><xs:documentation xml:lang="en">Max. spike rotation</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="moveDownRange" type="g_vector_rot_2">
                        <xs:annotation><xs:documentation xml:lang="en">Move down range</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="moveUpRange" type="g_vector_rot_2">
                        <xs:annotation><xs:documentation xml:lang="en">Move up range</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="rotAxis" type="g_int" default="3">
                        <xs:annotation><xs:documentation xml:lang="en">Rotation axis</xs:documentation></xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:all>
        <xs:attribute name="class" type="g_string" default="RotationAnimation">
            <xs:annotation><xs:documentation xml:lang="en">Animation class (RotationAnimation | RotationAnimationSpikes | ScrollingAnimation | ShakeAnimation)</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="minAlphaForTurnOff" type="g_float" default="0">
            <xs:annotation><xs:documentation xml:lang="en">Min. alpha for turn off (speed [0-1])</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="node" type="g_node_index">
            <xs:annotation><xs:documentation xml:lang="en">Node</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="rotAxis" type="g_float" default="2">
            <xs:annotation><xs:documentation xml:lang="en">Rotation axis</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="rotSpeed" type="g_angle" default="1">
            <xs:annotation><xs:documentation xml:lang="en">Rotation speed</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="scrollLength" type="g_float" default="1">
            <xs:annotation><xs:documentation xml:lang="en">(ScrollingAnimation) Scroll length</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="scrollSpeed" type="g_float" default="1">
            <xs:annotation><xs:documentation xml:lang="en">(ScrollingAnimation) Scroll speed</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="shaderComponentScale" type="g_vector_4" default="1 0 0 0">
            <xs:annotation><xs:documentation xml:lang="en">Shader parameter name</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="shaderParameterComponent" type="g_float" default="1">
            <xs:annotation><xs:documentation xml:lang="en">(ScrollingAnimation) Shader parameter component</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="shaderParameterName" type="g_string">
            <xs:annotation><xs:documentation xml:lang="en">Shader parameter name</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="shaderParameterPrevName" type="g_string" default="automatically calculated from #shaderParameterName">
            <xs:annotation><xs:documentation xml:lang="en">Prev Shader parameter name</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="shaking" type="g_vector_4" default="0 0 0 0">
            <xs:annotation><xs:documentation xml:lang="en">(ShakeAnimation) Shaking scale for shader parameters</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="speedFunc" type="g_string">
            <xs:annotation><xs:documentation xml:lang="en">Lua speed function</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="turnedOffPosition" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">(ScrollingAnimation) Target position while turned off</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="turnedOffRotation" type="g_angle">
            <xs:annotation><xs:documentation xml:lang="en">(RotationAnimation) Target rotation while turned off</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="turnedOffSubDivisions" type="g_float" default="1">
            <xs:annotation><xs:documentation xml:lang="en">Amount of sub divisions which have the same state</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="turnOffFadeTime" type="g_float" default="2">
            <xs:annotation><xs:documentation xml:lang="en">Turn off fade time</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="turnOnFadeTime" type="g_float" default="2">
            <xs:annotation><xs:documentation xml:lang="en">Turn on fade time</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="turnOnOffVariance" type="g_float">
            <xs:annotation><xs:documentation xml:lang="en">Turn off time variance</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="type" type="g_string">
            <xs:annotation><xs:documentation xml:lang="en">(ScrollingAnimation) Fill type name</xs:documentation></xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="EffectNode">
        <xs:choice maxOccurs="unbounded">
            <xs:element name="effectNode" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:choice maxOccurs="unbounded">
                        <xs:element name="motionPathEffect" minOccurs="0" maxOccurs="1">
                            <xs:complexType>
                                <xs:attribute name="delay" type="g_float" default="0">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Start and stop delay</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="density" type="g_float" default="1">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Density Scale</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="densityMaskFilename" type="g_string">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Custom Density Mask Texture</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="densityScale" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">(CultivatorMotionPathEffect) Density Scale</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="endFade" type="g_float" default="0.8">
                                    <xs:annotation><xs:documentation xml:lang="en">(WindrowerMotionPathEffect) End of fading depending on test area result</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="fadeOutScale" type="g_float" default="1">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Fade out speed multiplicator</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="fadeVisibilityMax" type="g_float" default="0">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Default fade visibility max. value</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="fadeVisibilityMin" type="g_float" default="1">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Default fade visibility min. value</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="inversedFadeOut" type="g_bool" default="false">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Using inversed fade in as fade out</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="isCultivatorSweepEffect" type="g_bool" default="false">
                                    <xs:annotation><xs:documentation xml:lang="en">(CultivatorMotionPathEffect) Is sweep effect</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="isLeft" type="g_bool" default="false">
                                    <xs:annotation><xs:documentation xml:lang="en">(WindrowerMotionPathEffect) Defines if rake is mounted on left or right side</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="isPickup" type="g_bool" default="false">
                                    <xs:annotation><xs:documentation xml:lang="en">(WindrowerMotionPathEffect) Defines if the effect is a pickup effect and width is adjusted by hiding rows instead of the fade value</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="maxDensitySpeed" type="g_float" default="8">
                                    <xs:annotation><xs:documentation xml:lang="en">(CultivatorMotionPathEffect) Speed at which the density is 1</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="maxReferenceVehicleSpeed" type="g_float" default="10">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) This speed represents speed &apos;1&apos; for effect</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="maxScaleSpeed" type="g_float" default="10">
                                    <xs:annotation><xs:documentation xml:lang="en">(PlowMotionPathEffect) Speed at which the effect reaches the max. scale</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="maxShapeScale" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Scale of single shapes at the end of the effect</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="maxVariationState" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">(CultivatorMotionPathEffect) Max. variation state</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="minDensity" type="g_float" default="0.5">
                                    <xs:annotation><xs:documentation xml:lang="en">(CultivatorMotionPathEffect) Min. Density</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="minFade" type="g_float" default="0">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Defines start fade value</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="minScaleOffset" type="g_float" default="-0.07">
                                    <xs:annotation><xs:documentation xml:lang="en">(PlowMotionPathEffect) Y Offset when the scale is at it&apos;s minimum</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="numRows" type="g_int" default="0">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Number of rows</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="rowLength" type="g_int" default="0">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Number of plants for each row</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="shapeScale" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Scale of single shapes</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="speedReferenceAnimation" type="g_string">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) This animation will be used for the effect speed</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="speedReferenceAnimationOffset" type="g_float" default="0">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Time offset to apply</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="speedScale" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Speed scale that is applied to effect speed defined in effect.xml or i3d file</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="startDelay" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Start delay</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="startFade" type="g_float" default="0.2">
                                    <xs:annotation><xs:documentation xml:lang="en">(WindrowerMotionPathEffect) Start of fading depending on test area result</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="stopDelay" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Stop delay</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="textureFilename" type="g_string">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Animation texture</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="textureRealWidth" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Real width of effect in meter with this texture</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="useVehicleSpeed" type="g_bool">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Use speed of vehicle as effect speed</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="verticalOffset" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Vertical offset of plants</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="visibilityX" type="g_vector_2" default="50 -50">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Visibility cut size X axis</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="visibilityY" type="g_vector_2" default="50 -50">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Visibility cut size Y axis</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="visibilityZ" type="g_vector_2" default="50 -50">
                                    <xs:annotation><xs:documentation xml:lang="en">(MotionPathEffect) Visibility cut size Z axis</xs:documentation></xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="particleSystem" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="delay" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">Activation delay</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="emitCountScale" type="g_float" default="1">
                                    <xs:annotation><xs:documentation xml:lang="en">Emit count scale</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="emitterShape" type="g_node_index">
                                    <xs:annotation><xs:documentation xml:lang="en">(WindrowerEffect) Emitter shape node</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="fadeInRange" type="g_vector_2">
                                    <xs:annotation><xs:documentation xml:lang="en">(WindrowerEffect) Fade in range</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="fadeOutRange" type="g_vector_2">
                                    <xs:annotation><xs:documentation xml:lang="en">(WindrowerEffect) Fade out range</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="lifespan" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">Lifespan</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="materialIndex" type="g_int" default="1">
                                    <xs:annotation><xs:documentation xml:lang="en">(WindrowerEffect) Particle type</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="materialType" type="g_string" default="same as particleType">
                                    <xs:annotation><xs:documentation xml:lang="en">(WindrowerEffect) Material type</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="particleType" type="g_string">
                                    <xs:annotation><xs:documentation xml:lang="en">(WindrowerEffect) Particle type</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="startTime" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">Start time</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="stopTime" type="g_float">
                                    <xs:annotation><xs:documentation xml:lang="en">Stop time</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="useEmitterVisibility" type="g_bool" default="true">
                                    <xs:annotation><xs:documentation xml:lang="en">use emitter shape visibility</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="worldSpace" type="g_bool" default="true">
                                    <xs:annotation><xs:documentation xml:lang="en">Is world space</xs:documentation></xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="testArea" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="heightNode" type="g_node_index">
                                    <xs:annotation><xs:documentation xml:lang="en">(WindrowerEffect) Test area height node</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="startNode" type="g_node_index">
                                    <xs:annotation><xs:documentation xml:lang="en">(WindrowerEffect) Test area start node</xs:documentation></xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="widthNode" type="g_node_index">
                                    <xs:annotation><xs:documentation xml:lang="en">(WindrowerEffect) Test area width node</xs:documentation></xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:choice>
                    <xs:attribute name="alignToWorldY" type="g_bool" default="false">
                        <xs:annotation><xs:documentation xml:lang="en">(ShaderPlaneEffect) Align Y axis to world Y</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="alignXAxisToWorldY" type="g_bool" default="false">
                        <xs:annotation><xs:documentation xml:lang="en">(ShaderPlaneEffect) Align X axis to world Y</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="alwaysVisibile" type="g_bool" default="false">
                        <xs:annotation><xs:documentation xml:lang="en">(ShaderPlaneEffect) Always visibile</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="controlPoint" type="g_vector_4" default="10 0.25 0 0">
                        <xs:annotation><xs:documentation xml:lang="en">(PipeEffect) Control point position</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="defaultFillType" type="g_string">
                        <xs:annotation><xs:documentation xml:lang="en">(ShaderPlaneEffect) Default fill type name</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="delay" type="g_float" default="0">
                        <xs:annotation><xs:documentation xml:lang="en">(ParticleEffect) Delay</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="dropOffset" type="g_float" default="0">
                        <xs:annotation><xs:documentation xml:lang="en">(WindrowerEffect) Drop offset</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="dynamicFillType" type="g_bool" default="false">
                        <xs:annotation><xs:documentation xml:lang="en">(ShaderPlaneEffect) Dynamic fill type</xs:documentation></xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="effectClass" type="g_string" default="ShaderPlaneEffect">
